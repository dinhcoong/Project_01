@model IEnumerable<mvcweb.Models.Category>

@{
    ViewBag.Title = "Danh mục sản phẩm";
}

<h2>Danh mục sản phẩm</h2>

<p>
    @Html.DisplayNameFor(model => model.CateDesciption)
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.CateName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CateDesciption)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                <a href="@Url.Action("GetbyCate","Categories",new { cateid = item.CategoryId })"> @Html.DisplayFor(modelItem => item.CateName) </a>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CateDesciption)
            </td>
            
        </tr>
    }

</table>
